<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Nuevo.Controllers.zonaControlador">
   <children>
      <ImageView fitHeight="603.0" fitWidth="617.0" layoutX="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/zonas.jpg" />
         </image>
      </ImageView>
      <Label layoutX="222.0" layoutY="28.0" text="MENÚ ZONAS" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="97.0" text="ESCOJA UNA OPCIÓN:" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="verZonas" layoutX="69.0" layoutY="128.0" mnemonicParsing="false" onAction="#verZonas" prefHeight="35.0" prefWidth="106.0" text="Ver zonas" />
      <Button layoutX="69.0" layoutY="183.0" mnemonicParsing="false" onAction="#crearZonas" prefHeight="35.0" prefWidth="106.0" text="Crear zonas" />
      <Button layoutX="69.0" layoutY="237.0" mnemonicParsing="false" onAction="#editarZona" prefHeight="35.0" prefWidth="106.0" text="Editar zonas" />
      <Button layoutX="69.0" layoutY="292.0" mnemonicParsing="false" onAction="#eliminarZona" prefHeight="35.0" prefWidth="106.0" text="Eliminar zonas" />
      <Button layoutX="509.0" layoutY="334.0" mnemonicParsing="false" onAction="#volver" prefHeight="35.0" prefWidth="61.0" text="Volver" />
   </children>
</AnchorPane>
