<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Nuevo.Controllers.sedeControlador">
   <children>
      <ImageView fitHeight="416.0" fitWidth="726.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/sedes.jpg" />
         </image>
      </ImageView>
      <Label layoutX="228.0" layoutY="28.0" text="MENÚ SEDES">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="49.0" layoutY="72.0" text="ESCOJA UNA OPCIÓN:">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="18.0" />
         </font></Label>
      <Button layoutX="70.0" layoutY="112.0" mnemonicParsing="false" onAction="#verSedes" prefHeight="34.0" prefWidth="105.0" text="Ver sedes" AnchorPane.leftAnchor="70.0" />
      <Button layoutX="70.0" layoutY="156.0" mnemonicParsing="false" onAction="#crearSede" prefHeight="34.0" prefWidth="105.0" text="Crear sede" AnchorPane.leftAnchor="70.0" />
      <Button layoutX="70.0" layoutY="203.0" mnemonicParsing="false" onAction="#editarSede" prefHeight="34.0" prefWidth="105.0" text="Editar sede" AnchorPane.leftAnchor="70.0" />
      <Button layoutX="70.0" layoutY="255.0" mnemonicParsing="false" onAction="#eliminarSedes" prefHeight="34.0" prefWidth="105.0" text="Eliminar sede" AnchorPane.leftAnchor="70.0" />
      <Button layoutX="517.0" layoutY="352.0" mnemonicParsing="false" onAction="#volver" text="Volver" />
      <Button layoutX="38.0" layoutY="306.0" mnemonicParsing="false" onAction="#agregarCurso" prefHeight="46.0" prefWidth="169.0" text="Agregar curso a una sede " />
   </children>
</AnchorPane>
